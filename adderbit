S_0xc9a7da0bf670 .scope module, "my_first_testbech" "my_first_testbech" 2 1;
 .timescale 0 0;
v0xc9a7da0d2f80_0 .var "a", 7 0;
v0xc9a7da0d3040_0 .var "b", 7 0;
v0xc9a7da0d3110_0 .net "sum", 8 0, L_0xc9a7da0d34c0;  1 drivers
S_0xc9a7da0bf800 .scope module, "ADDER1" "adder8bit" 2 12, 3 1 0, S_0xc9a7da0bf670;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "a";
    .port_info 1 /INPUT 8 "b";
    .port_info 2 /OUTPUT 9 "sum";
v0xc9a7da0bfa30_0 .net *"_ivl_0", 8 0, L_0xc9a7da0d3210;  1 drivers
L_0xf71503216018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0xc9a7da0d2990_0 .net *"_ivl_3", 0 0, L_0xf71503216018;  1 drivers
v0xc9a7da0d2a70_0 .net *"_ivl_4", 8 0, L_0xc9a7da0d3380;  1 drivers
