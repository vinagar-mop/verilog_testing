v0xc9a7da0d2d40_0 .net "b", 7 0, v0xc9a7da0d3040_0;  1 drivers
v0xc9a7da0d2e20_0 .net "sum", 8 0, L_0xc9a7da0d34c0;  alias, 1 drivers
L_0xc9a7da0d3210 .concat [ 8 1 0 0], v0xc9a7da0d2f80_0, L_0xf71503216018;
L_0xc9a7da0d3380 .concat [ 8 1 0 0], v0xc9a7da0d3040_0, L_0xf71503216060;
L_0xc9a7da0d34c0 .arith/sum 9, L_0xc9a7da0d3210, L_0xc9a7da0d3380;
    .scope S_0xc9a7da0bf670;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0xc9a7da0d2f80_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0xc9a7da0d3040_0, 0, 8;
    %end;
    .thread T_0;
    .scope S_0xc9a7da0bf670;
T_1 ;
    %vpi_call 2 20 "$monitor", "a=%d, b=%d, sum=%d", v0xc9a7da0d2f80_0, v0xc9a7da0d3040_0, v0xc9a7da0d3110_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0xc9a7da0bf670;
T_2 ;
    %delay 1, 0;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0xc9a7da0d2f80_0, 0, 8;
    %delay 1, 0;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0xc9a7da0d3040_0, 0, 8;
    %delay 1, 0;
    %pushi/vec4 3, 0, 8;
